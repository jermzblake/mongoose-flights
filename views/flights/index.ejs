<%- include ../partials/header.ejs %> 
<%- include ../partials/nav.ejs %>

<h1>Flights</h1>
<table class="table table-striped table-dark">
    <thead class="thead-light">
      <tr>
        <th>Airline</th>
        <th>Airport</th>
        <th>Flight No.</th>
        <th>Departs</th>
      </tr>
    </thead>
    <tbody>
        <% flights.forEach(function(f){ %> 
            <tr>
                <td><%= f.airline %></td>
                <td><%= f.airport %></td>
                <td><%= f.flightNo %></td>
                <td><%= f.departs %></td>
            </tr>
        <% }); %> 
    </tbody>
    </table>

<%- include ../partials/footer.ejs %>